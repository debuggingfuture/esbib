@INPROCEEDINGS{fujiwara2012efficient,
  author = {Fujiwara, Y. and Nakatsuji, M. and Yamamuro, T. and Shiokawa, H.
	and Onizuka, M.},
  title = {Efficient personalized pagerank with accuracy assurance},
  booktitle = {Proceedings of the 18th ACM SIGKDD international conference on Knowledge
	discovery and data mining},
  year = {2012},
  pages = {15--23},
  organization = {ACM},
  _h_pass1 = {2012.11.24},
  _h_pass2 = {2012.11.24},
  _h_pass3 = {undefined},
  _h_priority = {0},
  abstract = {Very good written paper! Good contribution on PPR computation!
	
	
	PPR query problems:
	
	 * Given restarting distribution (d) output score of a node x. 
	
	 * Given restarting distribution (d) output top K scored nodes.
	
	
	 * Given restarting distribution (d) output nodes with rank higher
	than epsilon.
	
	
	This paper achieves the above three problem with accuracy guarantee.
	Previous existing works focus more on approximate algorithms.},
  file = {:fujiwara2012-Efficient-personalized-pagerank-with-accuracy-assurance.pdf:PDF},
  owner = {hpl},
  review = {Key of single nodes query:
	
	 * PPR iteration form --> matrix inverse form. 
	
	 * Observation: Permutation matrix preserves inversion. 
	
	 * Observation: Proper permutation can result in sparse Q and R (QR
	decomposition) s.t. QR = I - (1-c)A'
	
	 * Q inverse is Q transpose; R inverse can be done by backward substitution.
	
	
	 * After Q R precomputation, the query for (x,d) can be done by simple
	inner product. Time cost is non-zero elements in Q and R.
	
	
	
	Top-k query:
	
	 * Estimate the lower bound of PPR on a BFS tree starting from nodes
	in d. O(n + m) time. 
	
	 * Sort nodes in descent order of lowerbound. 
	
	 * Use estimated lower bound and computed exact score to estimate
	an upper bound. 
	
	 * User upper bound to do early termination. 
	
	
	above-epsilon query:
	
	 * Similar to top-k. Estimate lowerbound and upper bound, then prune
	nodes. 
	
	 * Nodes with lowerbound above epsilon is directly in. 
	
	 * Nodes with upperbound below epsilon is directly out. 
	
	 * Compute for unsure nodes. 
	
	 * In the upperbound estimation stage, use lower bound (unlike the
	top-k query where exact score is computed).},
  timestamp = {2012.11.13}
}

